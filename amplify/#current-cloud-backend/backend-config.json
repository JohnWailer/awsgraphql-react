{
	"api": {
		"awsgraphqlapi": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "",
							"apiKeyExpirationDays": "365"
						}
					}
				}
			}
		}
	},
	"function": {
		"uberservicelambda": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "awsgraphqlapi",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		},
		"rnuberservicerestapi": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "awsgraphqlapi",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				},
				{
					"category": "function",
					"resourceName": "uberservicelambda",
					"attributes": [
						"Name"
					]
				}
			]
		}
	}
}